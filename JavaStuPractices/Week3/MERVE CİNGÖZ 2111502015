import java.util.Scanner;
public class EvdeOturma {

	public static void main(String[] args) {
		Scanner sc= new Scanner(System.in);
		System.out.println("Yas giriniz: ");
		int yas= sc.nextInt();
		sc.nextLine();
		
		System.out.println("Cinsiyet giriniz: ");
		String cinsiyet= sc.nextLine();
		
		System.out.println("Isminin bas harfi: ");
		String isim= sc.nextLine();
		
		
		if(yas<18 && yas>16 && cinsiyet.equals("erkek") && isim.equals("B")) {
			System.out.println("Evde otur");
		}
	}

}
